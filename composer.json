{
    "name": "vanderb/laravel-sluggable-redirect",
    "keywords": [
        "vanderb",
        "laravel-sluggable"
    ],
    "description": "Creates sluggable-history if slug created from laravel-sluggable-package by Spatie and redirects to current slug.",
    "license": "MIT",
    "authors": [
      {
        "name": "Freerk van Zeijl",
        "email": "freerk.vz@googlemail.com",
        "role": "Developer"
      }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-sluggable": "^3.0"
    },
    "autoload": {
      "psr-4": {
        "Vanderb\\SluggableRedirect\\": "src"
      }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vanderb\\SluggableRedirect\\SluggableRedirectServiceProvider"
            ]
        }
    }
  }
